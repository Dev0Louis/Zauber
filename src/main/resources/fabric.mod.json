{
  "schemaVersion": 1,
  "id": "zauber",
  "version": "${version}",
  "name": "Zauber",
  "description": "A Mod about discovering the power of Spells and Rituals.",
  "authors": [
    "Louis"
  ],
  "license": "MIT",
  "icon": "assets/zauber/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.louis.zauber.Zauber"
    ],
    "client": [
      "dev.louis.zauber.client.ZauberClient"
    ],
    "modmenu": [
      "dev.louis.zauber.client.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "zauber.mixins.json",
    {
      "config": "zauber.client.mixins.json",
      "environment": "client"
    }
  ],
  "accessWidener": "zauber.accesswidener",
  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": ">=1.21",
    "nebula": ">=6.0.1",
    "polymer-core": ">=0.6.2+1.20.2",
    "trinkets": ">=3.10.0"
  }
}
